generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ORCAMENTOS {
  id           Int     @id @default(autoincrement())
  NumOrc       Int
  DtEmissao    String
  Nome_cli     String
  ValorTotal   String
  DescSituacao String
  DataEntrega  String?
  Vend_Nome    String

  @@map("orcamento")
}

model Clientes {
  id           Int     @id @default(autoincrement())
  CodCli       Int?
  Nome_cli     String?
  Cnpj_cpf_cli String?
  Fone1_Cli    String
  Texto271     String?
  Cid_Cli      String?
  Uf_Cli       String?

  @@map("clientes")
}

model Produtos {
  id            Int     @id @default(autoincrement())
  Codigo        String
  Descricao   String
  codFornecedor String?
  Fabricante    String?

  @@map("produtos")
}
